   "source": [
    "# Solving:\n",
    "# 3x + y = 1\n",
    "# 2x + y = 1\n",
    "# (A)(X) = B -> X = (B)(A^-1)\n",
    "A = np.array([\n",
    "    [3, 1],\n",
    "    [2, 1]\n",
    "])\n",
    "B = np.array([1, 1])\n",
    "X = np.linalg.inv(A).dot(B)\n",
    "print(X)"
   ]
